    	public boolean autonomous;
    	{
    		AnalogGyro gyro = null;
    		// gyro.reset();
    		while (autonomous()) {
    			double angle = gyro1.getAngle(); // get current heading
    			Robot.drive(-1.0, -angle * Kp); // drive towards heading 0
    			Timer.delay(0.004);

    			Robot.drive(0.0, 0.0);
    		}
    	

